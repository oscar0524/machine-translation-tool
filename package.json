{
  "name": "machine-translation-tool",
  "version": "0.0.0",
  "main": "app/main.js",
  "scripts": {
    "ng": "ng",
    "ng:serve": "ng serve --poll 1000",
    "ng:build": "ng build",
    "ng:watch": "ng build --watch --configuration development",
    "ng:test": "ng test",
    "electron": "electron",
    "electron:tsc": "tsc -p tsconfig.serve.json",
    "electron:start": "ng build --base-href ./ && npm run electron:tsc && electron .",
    "electron:serve": "wait-on tcp:4200 && npm run electron:tsc && electron . --serve",
    "electron:build": "npm run build:prod && electron-builder build --publish=never",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:tsc && ng build --base-href ./",
    "build:prod": "npm run build -- -c production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/cdk": "^14.1.1",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/material": "^14.1.1",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@types/sqlite3": "^3.1.8",
    "chinese-conv": "^1.0.1",
    "ngx-scrollbar": "^10.0.1",
    "rxjs": "~7.5.0",
    "sqlite3": "^5.0.11",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^14.0.0",
    "@angular-devkit/build-angular": "^14.1.1",
    "@angular/cli": "~14.1.1",
    "@angular/compiler-cli": "^14.1.0",
    "@types/jasmine": "~4.0.0",
    "cheerio": "^1.0.0-rc.12",
    "electron": "^20.0.1",
    "electron-builder": "^23.3.3",
    "electron-debug": "^3.2.0",
    "electron-rebuild": "^3.2.9",
    "electron-reloader": "^1.2.3",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.7.2",
    "wait-on": "^6.0.1"
  }
}
